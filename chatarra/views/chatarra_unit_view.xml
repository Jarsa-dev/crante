<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <!-- Search View chatarra.unit -->
        <record id="chatarra_unit_search" model="ir.ui.view">
            <field name="name">chatarra.unit.search</field>
            <field name="model">chatarra.unit</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Unidades">
                    <field name='name'/>
                    <field name='client_id'/>
                    <field name='asignacion_id'/>
                    <field name='secretaria_id'/>
                    <field name='contacto_id'/>
                    <field name='gestor_id'/>
                    <group string="Filtrar por estado">
                        <filter string="Borrador" domain="[('state','=','borrador')]"/>
                        <filter string="Disponible" domain="[('state','=','disponible')]"/>
                        <filter string="Seleccionado para Asignacion" domain="[('state','=','por_asignar')]"/>
                        <filter string="Asignada" domain="[('state','=','asignada')]"/>
                        <filter string="Seleccionado para Envio" domain="[('state','=','seleccion')]"/>
                        <filter string="Enviado a SCT" domain="[('state','=','enviado')]"/>
                        <filter string="En Actualizacion" domain="[('state','=','en_actualizacion')]"/>
                        <filter string="Consulta" domain="[('state','=','consulta')]"/>
                        <filter string="Reposicion" domain="[('state','=','reposicion')]"/>
                        <filter string="Bloqueado" domain="[('state','=','bloqueado')]"/>
                        <filter string="Cita" domain="[('state','=','cita')]"/>
                        <filter string="Expediente Enviado" domain="[('state','=','exp_enviado')]"/>
                        <filter string="Chatarrizado" domain="[('state','=','chatarrizado')]"/>
                        <filter string="Certificado Recibo" domain="[('state','=','certificado')]"/>
                        <filter string="Baja" domain="[('state','=','baja')]"/>
                        <filter string="Cancelado" domain="[('state','=','cancelado')]"/>
                        <filter string="Desestimiento" domain="[('state','=','desestimiento')]"/>
                    </group>
                    <group  expand='0' string='Agrupar por...'>
                        <filter string='Estado' context="{'group_by' : 'state'}" />
                        <filter string='Cliente' context="{'group_by' : 'client_id'}" />
                        <filter string='Secretaria' context="{'group_by' : 'secretaria_id'}" />
                        <filter string='Contacto' context="{'group_by' : 'contacto_id'}" />
                        <filter string='Gestor' context="{'group_by' : 'gestor_id'}" />

                    </group>
                    <group string='Filtrar por fecha...'>
                        <filter string="Year"
                                name="Year"
                                domain="[('date','&lt;=', time.strftime('%%Y-%%m-%%d')),('date','&gt;=',time.strftime('%%Y-01-01'))]"
                                help="This Year" 
                                context="{'group_by':'date'}"/>
                        <filter string="Mes"
                                name="mes"
                                domain="[('date','&lt;=',(context_today()+relativedelta(day=31)).strftime('%%Y-%%m-%%d')),('date','&gt;=',(context_today()-relativedelta(day=1)).strftime('%%Y-%%m-%%d'))]"
                                help="Este Mes" 
                                context="{'group_by':'date'}"/>
                        <filter string="Semana"
                                separator="1"
                                name="semana"
                                domain="[('date','&lt;=', (context_today() + relativedelta(weeks=0,day=7, weekday=-1)).strftime('%%Y-%%m-%%d')),('date','&gt;=',(context_today() - relativedelta(weeks=1, weekday=0)).strftime('%%Y-%%m-%%d'))]"
                                help="Esta Semana" 
                                context="{'group_by':'date'}"/>
                    </group>
                </search>
            </field>
        </record>
        <!-- Vista de Arbol para Chatarras -->
    	<record model='ir.ui.view' id='chatarra_unit_tree'>
			<field name='name'>chatarra_unit.tree</field>
			<field name='model'>chatarra.unit</field>
			<field name='type'>tree</field>
			<field name='arch' type='xml'>
				<tree string='Chatarras' 
                    colors="gray:state=='reposicion';red:state=='borrador';green:state=='asignada';blue:state=='disponible';">
					<field name='fecha_registro'/>
                    <field name='name'/>
					<field name='serie'/>
                    <field name='state'/>
                    <button string="Disponible" 
                            name="action_disponible"
                            groups="chatarra.chatarra_group_disponible"
                            states="borrador"
                            type="object"
                            icon="gtk-go-forward"/>
                    <button string="Actualizada" 
                            name="action_actualizar"
                            groups="chatarra.chatarra_group_actualizar"
                            attrs="{'invisible':[('state','!=','en_actualizacion')]}"
                            type="object"
                            icon="gtk-dialog-apply"/>
                    <button string="Bloqueado" 
                            name="action_bloqueado"
                            groups="chatarra.chatarra_group_bloqueo"
                            attrs="{'invisible':[('state','!=','actualizada')]}"
                            type="object"
                            icon="gtk-dialog-authentication"/>
                    <button string="Programar Cita" 
                            name="%(chatarra.action_chatarra_cita_1)d"
                            groups="chatarra.chatarra_group_cita"
                            states="bloqueado"
                            type="action"
                            icon="stock_calendar"
                            context="{'default_unidad_id': active_id,}"/>
                    <button string="Chatarrizado" 
                            name="action_chatarrizado"
                            groups="chatarra.chatarra_group_chatarrizado"
                            states="exp_enviado"
                            type="object"
                            icon="gtk-apply"
                            class="oe_highlight"/>
                    <button string="Baja" 
                            name="action_baja"
                            groups="chatarra.chatarra_group_baja"
                            states="certificado"
                            type="object"
                            icon="gtk-apply"
                            class="oe_highlight"/>
				</tree>
			</field>
		</record>
        <!-- Vista de Arbol para Chatarras Enviadas a SCT -->
        <record model='ir.ui.view' id='chatarra_unit_enviado_sct_tree'>
            <field name='name'>chatarra_unit.tree</field>
            <field name='model'>chatarra.unit</field>
            <field name='type'>tree</field>
            <field name='arch' type='xml'>
                <tree string='Chatarras' 
                    colors="gray:state=='reposicion';red:state=='borrador';green:state=='asignada';blue:state=='disponible';">
                    <field name='client_id'/>
                    <field name='name'/>
                    <field name='serie'/>
                    <field name='secretaria_id'/>
                    <field name='contacto_id'/>
                    <field name='gestor_id'/>
                    <field name='paqueteria_id'/>
                    <field name='guia'/>
                    <field name='consulta'/>
                    <button string="Recibir Consulta" name="action_recibir_consulta" groups="chatarra.chatarra_group_recibir_sct" attrs="{'invisible':['|',('consulta','=',True),('state','!=','enviado')]}" type="object" class="oe_highlight" icon="gtk-apply"/>
                    <field name='tarjeta_circulacion'/>
                    <button string="Recibir TC" name="action_recibir_tarjeta" groups="chatarra.chatarra_group_recibir_sct" attrs="{'invisible':['|',('tarjeta_circulacion','=',True),('state','!=','enviado')]}" type="object" class="oe_highlight" icon="gtk-apply"/>
                    <field name='copia_tc'/>
                    <button string="Copia TC" name="%(chatarra.action_wizard_tarjeta)d" groups="chatarra.chatarra_group_recibir_sct" attrs="{'invisible':['|',('copia_tc','=',True),('state','!=','enviado')]}" type="action" class="oe_highlight" context="{'default_unit_id': active_id}" icon="gtk-apply"/>
                    <field name='state'/>
                </tree>
            </field>
        </record>
        <!-- Vista de Arbol para Chatarras en estado Cita -->
        <record model='ir.ui.view' id='chatarra_unit_cita_tree'>
            <field name='name'>chatarra_unit.tree</field>
            <field name='model'>chatarra.unit</field>
            <field name='type'>tree</field>
            <field name='arch' type='xml'>
                <tree string='Chatarras'>
                    <field name='rfc'/>
                    <field name='name'/>
                    <field name='serie'/>
                    <field name='chatarrera_id'/>
                    <field name='programacion_cita'/>
                    <field name='cita_anterior'/>
                    <field name='state'/>
                    <button string="Expediente Enviado"
                            name="action_exp_enviado"
                            groups="chatarra.chatarra_group_enviar_expediente"
                            states="cita"
                            type="object"
                            icon="gtk-revert-to-saved"
                            class="oe_highlight"/>
                    <button string="Reprogramar Cita"
                            name="%(chatarra.action_chatarra_cita_1)d"
                            groups="chatarra.chatarra_group_cita2"
                            states="cita"
                            type="action"
                            icon="stock_calendar"
                            context="{'default_unidad_id': active_id,}"/>
                </tree>
            </field>
        </record>
    	<!-- Vista de Formulario para Chatarras -->
    	<record model='ir.ui.view' id='chatarra_unit_form'>
    		<field name='name'>chatarra_unit.form</field>
    		<field name='model'>chatarra.unit</field>
    		<field name='type'>form</field>
    		<field name='arch' type='xml'>
    			<form string='Lista de Unidades'>
                    <header>
                        <button string="Disponible"
                                name="action_disponible"
                                groups="chatarra.chatarra_group_disponible"
                                states="borrador"
                                type="object"
                                icon="gtk-apply"
                                class="oe_highlight"/>
                        <button string="Recibir Consulta"
                                name="action_recibir_consulta"
                                groups="chatarra.chatarra_group_recibir_sct"
                                attrs="{'invisible':['|',('consulta','=',True),('state','!=','enviado')]}"
                                type="object"
                                class="oe_highlight"/>
                        <button string="Recibir TC"
                                name="action_recibir_tarjeta" 
                                groups="chatarra.chatarra_group_recibir_sct"
                                attrs="{'invisible':['|',('tarjeta_circulacion','=',True),('state','!=','enviado')]}"
                                type="object"
                                class="oe_highlight"/>
                        <button string="Copia TC"
                                name="%(chatarra.action_wizard_tarjeta)d" 
                                groups="chatarra.chatarra_group_recibir_sct" 
                                attrs="{'invisible':['|',('copia_tc','=',True),('state','!=','enviado')]}"
                                type="action"
                                class="oe_highlight"
                                context="{'default_unit_id': active_id}"/>
                        <button string="Actualizada" 
                                name="action_actualizar"
                                groups="chatarra.chatarra_group_actualizar"
                                attrs="{'invisible':[('state','!=','en_actualizacion')]}"
                                type="object"
                                icon="gtk-dialog-apply"/>
                        <button string="Bloqueado" 
                                name="action_bloqueado"
                                groups="chatarra.chatarra_group_bloqueo"
                                attrs="{'invisible':[('state','!=','actualizada')]}"
                                type="object"
                                icon="gtk-dialog-authentication"/>
                        <button string="Programar Cita"
                                name="%(chatarra.action_chatarra_cita_1)d" 
                                groups="chatarra.chatarra_group_cita"
                                states="bloqueado"
                                type="action"
                                icon="stock_calendar"
                                class="oe_highlight"
                                context="{'default_unidad_id': active_id,}"/>
                        <button string="Expediente Enviado"
                                name="action_exp_enviado" 
                                groups="chatarra.chatarra_group_enviar_expediente"
                                states="cita"
                                type="object"
                                icon="gtk-revert-to-saved"
                                class="oe_highlight"/>
                        <button string="Reprogramar Cita"
                                name="%(chatarra.action_chatarra_cita_1)d" 
                                groups="chatarra.chatarra_group_cita2"
                                states="cita"
                                type="action"
                                icon="stock_calendar"
                                context="{'default_unidad_id': active_id,}"/>
                        <button string="Chatarrizado"
                                name="action_chatarrizado" 
                                groups="chatarra.chatarra_group_chatarrizado"
                                states="exp_enviado"
                                type="object"
                                icon="gtk-apply"
                                class="oe_highlight"/>
                        <button string="Certificado"
                                name="%(chatarra.action_wizard_chatarra_certificado)d" 
                                groups="chatarra.chatarra_group_certificado"
                                states="chatarrizado"
                                type="action"
                                icon="gtk-apply"
                                class="oe_highlight"
                                context="{'default_unit_id': active_id, 'default_fecha': programacion_cita}"/>
                        <button string="Baja"
                                name="action_baja" 
                                groups="chatarra.chatarra_group_baja"
                                states="certificado"
                                type="object"
                                icon="gtk-apply"
                                class="oe_highlight"/>
                        <button string="Desestimiento"
                                name="action_cancelado" 
                                groups="chatarra.chatarra_group_desestimiento"
                                states="en_actualizacion,bloqueado,cita,exp_enviado,chatarrizado,certificado,baja"
                                type="object"
                                icon="gtk-close"/>
                        <button string="Desestimiento y Reposicion"
                                name="%(chatarra.action_chatarra_reposicion)d" 
                                groups="chatarra.chatarra_group_desestimiento_reposicion"
                                states="en_actualizacion,bloqueado,cita,exp_enviado,chatarrizado,certificado"
                                type="action"
                                icon="gtk-convert"
                                context="{'default_unidad_anterior_id': active_id,}"/>
                        <button string="Reponer"
                                name="%(chatarra.action_chatarra_reposicion)d" 
                                groups="chatarra.chatarra_group_reposiciones"
                                states="asignada"
                                type="action"
                                icon="gtk-convert"
                                context="{'default_unidad_anterior_id': active_id,}"/>
                        <button string="Desasignar"
                                name="action_desasignar" 
                                groups="chatarra.chatarra_group_desasingar"
                                states="asignada,enviado"
                                type="object"
                                icon="gtk-ok"/>
                        <!-- BOORRAR ESTE BOTON SOLO PARA PRUEBAS INICIO-->
                        <button string="Volver a Borrador" 
                                name="action_borrador"
                                type="object"/>
                        <!-- BOORRAR ESTE BOTON SOLO PARA PRUEBAS FIN-->
                        <field
                                name='state' widget="statusbar" statusbar_visible="borrador, disponible, asignada, reposicion"/>
                    </header>
    				<sheet>
    					<h1>
    						<label string='Catalogo de Unidades'/>
    					</h1>
    					<group col='4'>
    						<field name='name' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                            <field name='fecha_registro' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                            <field name='supplier_id' domain="[('supplier','=',True),('is_company','=',True),('categoria','=','proveedor_visual')]" context="{'default_customer': False, 'default_supplier': True, 'default_is_company': True, 'default_categoria': 'proveedor_visual'}" attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                            <field name='importe' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                            <field name='client_id' attrs="{'invisible':[('client_id','=',False)],'readonly': [('state', '!=', 'borrador')]}"/>
    					</group>
    					<notebook>
    						<page string='Datos de la Unidad'>
    							<group col='4'>
 									<field name='serie' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
 									<field name='marca_id' widget='selection' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='modelo' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
 									<field name='clase' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='no_ejes' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
 									<field name='tipo_id' widget='selection' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='no_llantas' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
 									<field name='motor' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='combustible' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='cap_toneladas' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='cap_personas' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='cap_litros' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='peso_vehicular' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='alto' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='ancho' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='largo' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='eje_direccional' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='eje_motriz' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='eje_carga' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='tipo_placa' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='modalidad' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='permiso_ruta' attrs="{'readonly': [('state', '!=', 'borrador')]}"/> 
                                    <field name='fecha_sustitucion' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='lugar_exp' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='fecha_exp' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='tramite' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='observacion' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='color_placa' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='folio_tarjeta' attrs="{'invisible':[('folio_tarjeta','=',False)]}"/>
                                    <field name='fecha_tarjeta' attrs="{'invisible':[('fecha_tarjeta','=',False)]}"/>
                                    <field name='folio_modalidad' attrs="{'invisible':[('folio_modalidad','=','0')]}"/>
                                </group>
    						</page>
                            <page string='Datos del Propietario Ant.'>
                                <group col='4'>
                                    <field name='rfc' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='propietario_anterior' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='calle' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='numero' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='colonia' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='codigo_postal' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='ciudad' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                </group>
                            </page>
                            <page string='Documentacion' attrs="{'invisible': ['|','|',('state','in','borrador')]}">
                                <group>
                                    <separator string="Documentos" coslpan="4"/>
                                    <field name='document_ids' colspan="4" nolabel="1">
                                        <tree string='Documentacion' editable="top">
                                            <field name='name'/>
                                            <field name='unit_id' readonly="True"/>
                                            <field name='state'/>
                                            <button name="action_no_requerido" 
                                                attrs="{'invisible':['|','|','|','|','|','|','|',('name','=','visual'),('name','=','carta'),('name','=','consulta'),('name','=','copia_tc'),('name','=','foto_frente'),('name','=','foto_chasis'),('name','=','foto_motor'),('state','!=','pendiente')]}" 
                                                string="Completo" 
                                                groups="chatarra.chatarra_group_documentos"
                                                type="object" 
                                                icon="gtk-cancel"/>
                                            <button name="action_completo" groups="chatarra.chatarra_group_documentos" states="pendiente" string="Completo" type="object" icon="gtk-apply"/>
                                        </tree>
                                    </field>
                                </group>
                            </page>
                            <page string='Referencias' attrs="{'invisible':[('state','=', 'borrador')]}">
                                <group col='4'>
                                    <field name='factura_proveedor_id' attrs="{'invisible':[('factura_proveedor_id','=', False)]}"/>
                                    <field name='asignacion_id' attrs="{'invisible':[('asignacion_id','=', False)]}"/>
                                    <field name='asignacion2_id' attrs="{'invisible':[('asignacion2_id','=', False)]}"/>
                                    <field name='asignacion3_id' attrs="{'invisible':[('asignacion3_id','=', False)]}"/>
                                    <field name='reposicion_id' attrs="{'invisible':[('reposicion_id','=', False)]}"/>
                                    <field name='facturado' attrs="{'invisible':[('facturado','=', False)]}"/>
                                    <field name='factura_id' attrs="{'invisible':[('factura_id','=', False)]}"/>
                                    <field name='repuesta_id' attrs="{'invisible':[('repuesta_id','=', False)]}"/>
                                    <field name='sustituye_id' attrs="{'invisible':[('sustituye_id','=', False)]}"/>
                                    <field name='envio_id' attrs="{'invisible':[('envio_id','=', False)]}"/>
                                    <field name='guia' attrs="{'invisible':[('guia','=', False)]}"/>
                                    <field name='secretaria_id' attrs="{'invisible':[('secretaria_id','=', False)]}"/>
                                    <field name='contacto_id' attrs="{'invisible':[('contacto_id','=', False)]}"/>
                                    <field name='gestor_id' attrs="{'invisible':[('gestor_id','=', False)]}"/>
                                    <field name='paqueteria_id' attrs="{'invisible':[('paqueteria_id','=', False)]}"/>
                                    <field name='consulta' attrs="{'invisible':[('consulta','=', False)]}"/>
                                    <field name='tarjeta_circulacion' attrs="{'invisible':[('tarjeta_circulacion','=', False)]}"/>
                                    <field name='copia_tc' attrs="{'invisible':[('copia_tc','=', False)]}"/>
                                    <field name='chatarrera_id' attrs="{'invisible':[('chatarrera_id','=', False)]}"/>
                                    <field name='programacion_cita' attrs="{'invisible':[('programacion_cita','=', False)]}"/>
                                    <field name='cita_anterior' attrs="{'invisible':[('cita_anterior','=', False)]}"/>
                                    <field name='cita_anterior2' attrs="{'invisible':[('cita_anterior2','=', False)]}"/>
                                    <field name='certificado' attrs="{'invisible':[('certificado','=', False)]}"/>
                                    <field name='expediente_completo' attrs="{'invisible':[('expediente_completo','=', False)]}"/>
                                </group>
                            </page>
                            <page string='Log'>
                                <group col='4'>
                                    <field name='disponible_por' attrs="{'invisible':[('disponible_por','=', False)]}"/>
                                    <field name='fecha_disponible' attrs="{'invisible':[('fecha_disponible','=', False)]}"/>
                                    <field name='asignada_por' attrs="{'invisible':[('asignada_por','=', False)]}"/>
                                    <field name='fecha_asignada' attrs="{'invisible':[('fecha_asignada','=', False)]}"/>
                                    <field name='facturado_por' attrs="{'invisible':[('facturado_por','=', False)]}"/>
                                    <field name='fecha_facturado' attrs="{'invisible':[('fecha_facturado','=', False)]}"/>
                                    <field name='desasignado_por' attrs="{'invisible':[('desasignado_por','=', False)]}"/>
                                    <field name='fecha_desasignado' attrs="{'invisible':[('fecha_desasignado','=', False)]}"/>
