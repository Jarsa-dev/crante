<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <!-- Search View chatarra.unit -->
        <record id="chatarra_unit_search" model="ir.ui.view">
            <field name="name">chatarra.unit.search</field>
            <field name="model">chatarra.unit</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Unidades">
                   <filter string="Disponibles" icon="terp-gtk-media-pause" domain="[('state','=','disponible')]"/>
                   <filter string="Borrador" icon="terp-gtk-media-pause" domain="[('state','=','borrador')]"/>
                   <filter string="Asignada" icon="terp-gtk-media-pause" domain="[('state','=','asignada')]"/>
                   <filter string="Enviada a SCT" icon="terp-gtk-media-pause" domain="[('state','=','enviado')]"/>
                   <separator orientation="vertical"/>
                   <group  expand='0' string='Agrupar por...'>
                       <filter string='Estado' icon="terp-gdu-smart-failing" domain="[]" context="{'group_by' : 'state'}" />
                   </group>
                </search>
            </field>
        </record>
        <!-- Vista de Arbol para Chatarras -->
    	<record model='ir.ui.view' id='chatarra_unit_tree'>
			<field name='name'>chatarra_unit.tree</field>
			<field name='model'>chatarra.unit</field>
			<field name='type'>tree</field>
			<field name='arch' type='xml'>
				<tree string='Chatarras' 
                    colors="gray:state=='reposicion';red:state=='borrador';green:state=='asignada';blue:state=='disponible';">
					<field name='vat'/>
                    <field name='name'/>
					<field name='serie'/>
                    <field name='state'/>
                    <button name="action_disponible" groups="chatarra.chatarra_group_disponible" states="borrador" string="Disponible" type="object" icon="gtk-go-forward"/>
                    <button name="action_bloqueado" groups="chatarra.chatarra_group_bloqueo" attrs="{'invisible':[('state','!=','recibido')]}" string="Bloqueado" type="object" icon="gtk-dialog-authentication"/>
                    <button name="%(chatarra.action_chatarra_cita_1)d" groups="chatarra.chatarra_group_cita" states="bloqueado" string="Programar Cita" type="action" icon="stock_calendar" context="{'default_unidad_id': active_id,}"/>
				</tree>
			</field>
		</record>
        <!-- Vista de Arbol para Chatarras Enviadas a SCT -->
        <record model='ir.ui.view' id='chatarra_unit_enviado_sct_tree'>
            <field name='name'>chatarra_unit.tree</field>
            <field name='model'>chatarra.unit</field>
            <field name='type'>tree</field>
            <field name='arch' type='xml'>
                <tree string='Chatarras' 
                    colors="gray:state=='reposicion';red:state=='borrador';green:state=='asignada';blue:state=='disponible';">
                    <field name='client_id'/>
                    <field name='name'/>
                    <field name='serie'/>
                    <field name='secretaria_id'/>
                    <field name='paqueteria_id'/>
                    <field name='guia'/>
                    <field name='consulta'/>
                    <button name="action_recibir_consulta" groups="chatarra.chatarra_group_recibir_sct" attrs="{'invisible':['|',('consulta','=',True),('state','!=','enviado')]}" string="Recibir Consulta" type="object" class="oe_highlight" icon="gtk-apply"/>
                    <field name='tarjeta_circulacion'/>
                    <button name="action_recibir_tarjeta" groups="chatarra.chatarra_group_recibir_sct" attrs="{'invisible':['|',('tarjeta_circulacion','=',True),('state','!=','enviado')]}" string="Recibir TC" type="object" class="oe_highlight" icon="gtk-apply"/>
                    <field name='copia_tc'/>
                    <button name="%(chatarra.action_wizard_tarjeta)d" groups="chatarra.chatarra_group_recibir_sct" attrs="{'invisible':['|',('copia_tc','=',True),('state','!=','enviado')]}" string="Copia TC" type="action" class="oe_highlight" context="{'default_unit_id': active_id}" icon="gtk-apply"/>
                    <field name='state'/>
                </tree>
            </field>
        </record>
        <!-- Vista de Arbol para Chatarras en estado Cita -->
        <record model='ir.ui.view' id='chatarra_unit_cita_tree'>
            <field name='name'>chatarra_unit.tree</field>
            <field name='model'>chatarra.unit</field>
            <field name='type'>tree</field>
            <field name='arch' type='xml'>
                <tree string='Chatarras'>
                    <field name='vat'/>
                    <field name='name'/>
                    <field name='serie'/>
                    <field name='chatarrera_id'/>
                    <field name='programacion_cita'/>
                    <field name='cita_anterior'/>
                    <field name='state'/>
                    <button name="action_exp_enviado" groups="chatarra.chatarra_group_enviar_expediente" states="cita" string="Expediente Enviado" type="object" icon="gtk-revert-to-saved" class="oe_highlight"/>
                    <button  name="%(chatarra.action_chatarra_cita_1)d" groups="chatarra.chatarra_group_cita2" states="cita" string="Reprogramar Cita" type="action" icon="stock_calendar" context="{'default_unidad_id': active_id,}"/>
                </tree>
            </field>
        </record>
    	<!-- Vista de Formulario para Chatarras -->
    	<record model='ir.ui.view' id='chatarra_unit_form'>
    		<field name='name'>chatarra_unit.form</field>
    		<field name='model'>chatarra.unit</field>
    		<field name='type'>form</field>
    		<field name='arch' type='xml'>
    			<form string='Lista de Unidades'>
                    <header>
                        <button name="action_disponible" groups="chatarra.chatarra_group_disponible" states="borrador" string="Disponible" type="object" icon="gtk-apply" class="oe_highlight"/>
                        <button name="action_completo_unidad" groups="chatarra.chatarra_group_documentos" states="asignada" string="Expediente Completo" class="oe_highlight" type="object" icon="gtk-apply"/>
                        <button name="action_recibir_consulta" groups="chatarra.chatarra_group_recibir_sct" attrs="{'invisible':['|',('consulta','=',True),('state','!=','enviado')]}" string="Recibir Consulta" type="object" class="oe_highlight"/>
                        <button name="action_recibir_tarjeta" groups="chatarra.chatarra_group_recibir_sct" attrs="{'invisible':['|',('tarjeta_circulacion','=',True),('state','!=','enviado')]}" string="Recibir TC" type="object" class="oe_highlight"/>
                        <button name="%(chatarra.action_wizard_tarjeta)d" groups="chatarra.chatarra_group_recibir_sct" attrs="{'invisible':['|',('copia_tc','=',True),('state','!=','enviado')]}" string="Copia TC" type="action" class="oe_highlight" context="{'default_unit_id': active_id}"/>
                        <button name="action_bloqueado" groups="chatarra.chatarra_group_bloqueo" states="recibido" string="Bloqueado" type="object" icon="gtk-dialog-authentication" class="oe_highlight"/>
                        <button  name="%(chatarra.action_chatarra_cita_1)d" groups="chatarra.chatarra_group_cita" states="bloqueado" string="Programar Cita" type="action" icon="stock_calendar" class="oe_highlight" context="{'default_unidad_id': active_id,}"/>
                        <button name="action_exp_enviado" groups="chatarra.chatarra_group_enviar_expediente" states="cita" string="Expediente Enviado" type="object" icon="gtk-revert-to-saved" class="oe_highlight"/>
                        <button  name="%(chatarra.action_chatarra_cita_1)d" groups="chatarra.chatarra_group_cita2" states="cita" string="Reprogramar Cita" type="action" icon="stock_calendar" context="{'default_unidad_id': active_id,}"/>
                        <button name="action_chatarrizado" groups="chatarra.chatarra_group_chatarrizado" states="exp_enviado" string="Chatarrizado" type="object" icon="gtk-apply" class="oe_highlight"/>
                        <button name="%(chatarra.action_chatarra_certificado)d" groups="chatarra.chatarra_group_certificado" states="chatarrizado" string="Certificado" type="action" icon="gtk-apply" class="oe_highlight" context="{'default_unit_id': active_id, 'default_fecha': programacion_cita}"/>
                        <button name="action_baja" groups="chatarra.chatarra_group_baja" states="certificado" string="Baja" type="object" icon="gtk-apply" class="oe_highlight"/>
                        <button name="action_cancelado" groups="chatarra.chatarra_group_desestimiento" states="recibido,enviado_agencia,bloqueado,cita,exp_enviado,chatarrizado,certificado,baja" string="Desestimiento" type="object" icon="gtk-close"/>
                        <button name="%(chatarra.action_chatarra_reposicion)d" groups="chatarra.chatarra_group_desestimiento_reposicion" states="recibido,enviado_agencia,bloqueado,cita,exp_enviado,chatarrizado,certificado" string="Desestimiento y Reposicion" type="action" icon="gtk-convert" context="{'default_unidad_anterior_id': active_id,}"/>
                        <button  name="%(chatarra.action_chatarra_reposicion)d" groups="chatarra.chatarra_group_reposiciones" states="asignada,completo" string="Reponer" type="action" icon="gtk-convert" context="{'default_unidad_anterior_id': active_id,}"/>
                        <button  name="action_desasignar" groups="chatarra.chatarra_group_desasingar" states="asignada,elaboracion,completo,enviado" string="Desasignar" type="object" icon="gtk-ok"/>
                        <field name='state' widget="statusbar" statusbar_visible="borrador,disponible,asignada,reposicion"/>
                    </header>
    				<sheet>
    					<h1>
    						<label string='Catalogo de Unidades'/>
    					</h1>
    					<group col='4'>
    						<field name='name' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                            <field name='supplier_id' domain="[('supplier','=',True),('is_company','=',True)]" context="{'default_customer': False, 'default_supplier': True, 'default_is_company': True,}" attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
    						<field name='propietario_id' on_change='onchange_vat(propietario_id)' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
    						<field name='vat' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                            <field name='client_id' attrs="{'invisible':[('client_id','=',False)],'readonly': [('state', '!=', 'borrador')]}"/>
    					</group>
    					<notebook>
    						<page string='Datos de la Unidad'>
    							<group col='4'>
 									<field name='serie' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
 									<field name='marca' widget='selection' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
 									<field name='clase' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
 									<field name='tipo' widget='selection' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
 									<field name='motor' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='color_placa' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='tipo_placa' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
 									<field name='combustible' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
 									<field name='peso_vehicular' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='modelo' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='reg_fed' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='modalidad' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='no_ejes' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='no_llantas' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='cap_litros' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='cap_toneladas' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='cap_personas' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='alto' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='ancho' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='largo' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='eje_direccional' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='eje_motriz' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='eje_carga' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='permiso_ruta' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>                  
                                    <field name='lugar_exp' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='fecha_exp' attrs="{'readonly': [('state', '!=', 'borrador')]}"/>
                                    <field name='folio_tarjeta'/>
                                    <field name='fecha_tarjeta'/>
                                    <field name='folio_modalidad'/>
                                </group>
    						</page>
                            <page string='Documentacion' attrs="{'invisible': ['|','|',('state','in','borrador')]}">
                                <group>
                                    <separator string="Documentos" coslpan="4"/>
                                    <field name='document_ids' colspan="4" nolabel="1">
                                        <tree string='Documentacion' editable="top">
                                            <field name='name'/>
                                            <field name='unit_id' readonly="True"/>
                                            <field name='state'/>
                                            <button name="action_no_requerido" 
                                                attrs="{'invisible':['|','|','|','|','|','|','|',('name','=','visual'),('name','=','carta'),('name','=','consulta'),('name','=','copia_tc'),('name','=','foto_frente'),('name','=','foto_chasis'),('name','=','foto_motor'),('state','!=','pendiente')]}" 
                                                string="Completo" 
                                                groups="chatarra.chatarra_group_documentos"
                                                type="object" 
                                                icon="gtk-cancel"/>
                                            <button name="action_completo" groups="chatarra.chatarra_group_documentos" states="pendiente" string="Completo" type="object" icon="gtk-apply"/>
                                        </tree>
                                    </field>
                                </group>
                            </page>
                            <page string='Referencias' attrs="{'invisible':[('state','=', 'borrador')]}">
                                <group col='4'>
                                    <field name='factura_proveedor_id' attrs="{'invisible':[('factura_proveedor_id','=', False)]}"/>
                                    <field name='asignacion_id' attrs="{'invisible':[('asignacion_id','=', False)]}"/>
                                    <field name='asignacion2_id' attrs="{'invisible':[('asignacion2_id','=', False)]}"/>
                                    <field name='asignacion3_id' attrs="{'invisible':[('asignacion3_id','=', False)]}"/>
                                    <field name='reposicion_id' attrs="{'invisible':[('reposicion_id','=', False)]}"/>
                                    <field name='facturado' attrs="{'invisible':[('facturado','=', False)]}"/>
                                    <field name='factura_id' attrs="{'invisible':[('factura_id','=', False)]}"/>
                                    <field name='repuesta_id' attrs="{'invisible':[('repuesta_id','=', False)]}"/>
                                    <field name='sustituye_id' attrs="{'invisible':[('sustituye_id','=', False)]}"/>
                                    <field name='envio_id' attrs="{'invisible':[('envio_id','=', False)]}"/>
                                    <field name='guia' attrs="{'invisible':[('guia','=', False)]}"/>
                                    <field name='secretaria_id' attrs="{'invisible':[('secretaria_id','=', False)]}"/>
                                    <field name='paqueteria_id' attrs="{'invisible':[('paqueteria_id','=', False)]}"/>
                                    <field name='consulta' attrs="{'invisible':[('consulta','=', False)]}"/>
                                    <field name='tarjeta_circulacion' attrs="{'invisible':[('tarjeta_circulacion','=', False)]}"/>
                                    <field name='copia_tc' attrs="{'invisible':[('copia_tc','=', False)]}"/>
                                    <field name='chatarrera_id' attrs="{'invisible':[('chatarrera_id','=', False)]}"/>
                                    <field name='programacion_cita' attrs="{'invisible':[('programacion_cita','=', False)]}"/>
                                    <field name='cita_anterior' attrs="{'invisible':[('cita_anterior','=', False)]}"/>
                                    <field name='cita_anterior2' attrs="{'invisible':[('cita_anterior2','=', False)]}"/>
                                </group>
                            </page>
                            <page string='Log'>
                                <group col='4'>
                                    <field name='disponible_por' attrs="{'invisible':[('disponible_por','=', False)]}"/>
                                    <field name='fecha_disponible' attrs="{'invisible':[('fecha_disponible','=', False)]}"/>
                                    <field name='asignada_por' attrs="{'invisible':[('asignada_por','=', False)]}"/>
                                    <field name='fecha_asignada' attrs="{'invisible':[('fecha_asignada','=', False)]}"/>
                                    <field name='facturado_por' attrs="{'invisible':[('facturado_por','=', False)]}"/>
                                    <field name='fecha_facturado' attrs="{'invisible':[('fecha_facturado','=', False)]}"/>
                                    <field name='desasignado_por' attrs="{'invisible':[('desasignado_por','=', False)]}"/>
                                    <field name='fecha_desasignado' attrs="{'invisible':[('fecha_desasignado','=', False)]}"/>
                                    <field name='desasignado2_por' attrs="{'invisible':[('desasignado2_por','=', False)]}"/>
                                    <field name='fecha_desasignado2' attrs="{'invisible':[('fecha_desasignado2','=', False)]}"/>
                                    <field name='completo_por' attrs="{'invisible':[('completo_por','=', False)]}"/>
                                    <field name='fecha_completo' attrs="{'invisible':[('fecha_completo','=', False)]}"/>
                                    <field name='enviado_por' attrs="{'invisible':[('enviado_por','=', False)]}"/>
                                    <field name='fecha_enviado' attrs="{'invisible':[('fecha_enviado','=', False)]}"/>
                                    <field name='copia_tc_por' attrs="{'invisible':[('copia_tc_por','=', False)]}"/>
                                    <field name='fecha_copia_tc' attrs="{'invisible':[('fecha_copia_tc','=', False)]}"/>
                                    <field name='consulta_por' attrs="{'invisible':[('consulta_por','=', False)]}"/>
                                    <field name='fecha_consulta' attrs="{'invisible':[('fecha_consulta','=', False)]}"/>
                                    <field name='tc_por' attrs="{'invisible':[('tc_por','=', False)]}"/>
                                    <field name='fecha_tc' attrs="{'invisible':[('fecha_tc','=', False)]}"/>
                                    <field name='recibido_por' attrs="{'invisible':[('recibido_por','=', False)]}"/>
                                    <field name='fecha_recibido' attrs="{'invisible':[('fecha_recibido','=', False)]}"/>
                                    <field name='bloqueado_por' attrs="{'invisible':[('bloqueado_por','=', False)]}"/>
                                    <field name='fecha_bloqueado' attrs="{'invisible':[('fecha_bloqueado','=', False)]}"/>
                                    <field name='cita_por' attrs="{'invisible':[('cita_por','=', False)]}"/>
                                    <field name='fecha_cita' attrs="{'invisible':[('fecha_cita','=', False)]}"/>
                                    <field name='cita_reprogramada_por' attrs="{'invisible':[('cita_reprogramada_por','=', False)]}"/>
                                    <field name='fecha_cita_reprogramada' attrs="{'invisible':[('fecha_cita_reprogramada','=', False)]}"/>
                                    <field name='cita_reprogramada2_por' attrs="{'invisible':[('cita_reprogramada2_por','=', False)]}"/>
                                    <field name='fecha_cita_reprogramada2' attrs="{'invisible':[('fecha_cita_reprogramada2','=', False)]}"/>
                                    <field name='exp_enviado_por' attrs="{'invisible':[('exp_enviado_por','=', False)]}"/>
                                    <field name='fecha_exp_enviado' attrs="{'invisible':[('fecha_exp_enviado','=', False)]}"/>
                                    <field name='chatarrizado_por' attrs="{'invisible':[('chatarrizado_por','=', False)]}"/>
                                    <field name='fecha_chatarrizado' attrs="{'invisible':[('fecha_chatarrizado','=', False)]}"/>
                                    <field name='certificado_por' attrs="{'invisible':[('certificado_por','=', False)]}"/>
                                    <field name='fecha_certificado' attrs="{'invisible':[('fecha_certificado','=', False)]}"/>
                                    <field name='baja_por' attrs="{'invisible':[('baja_por','=', False)]}"/>
                                    <field name='fecha_baja' attrs="{'invisible':[('fecha_baja','=', False)]}"/>
                                    <field name='cancelado_por' attrs="{'invisible':[('cancelado_por','=', False)]}"/>
                                    <field name='fecha_cancelado' attrs="{'invisible':[('fecha_cancelado','=', False)]}"/>
                                    <field name='desestimiento_por' attrs="{'invisible':[('desestimiento_por','=', False)]}"/>
                                    <field name='fecha_desestimiento' attrs="{'invisible':[('fecha_desestimiento','=', False)]}"/>
                                    <field name='reposicion_por' attrs="{'invisible':[('reposicion_por','=', False)]}"/>
                                    <field name='fecha_reposicion' attrs="{'invisible':[('fecha_reposicion','=', False)]}"/>
                                </group>
                            </page>
    					</notebook>
    				</sheet>
    			</form>
    		</field>
    	</record>
       <!-- Accion Chatarra/Unidades/Inventario -->
        <record id='action_chatarra_list' model='ir.actions.act_window'>
        	<field name='name'>Inventario</field>
            <field name="view_id" ref="chatarra_unit_tree"/>
        	<field name='res_model'>chatarra.unit</field>
        	<field name='view_type'>form</field>
        	<field name='view_mode'>tree,form</field>
        </record>
        <!-- Accion Chatarra/Unidades/Disponibles -->
        <record id='action_chatarra_disponibles' model='ir.actions.act_window'>
            <field name='name'>Unidades Disponibles</field>
            <field name='res_model'>chatarra.unit</field>
            <field name="view_id" ref="chatarra_unit_tree"/>
            <field name="domain">[('state','=','disponible')]</field>
            <field name='view_type'>form</field>
            <field name='view_mode'>tree,form</field>
        </record>
        <!-- Accion Chatarra/Unidades/Asignadas -->
        <record id='action_chatarra_asignadas' model='ir.actions.act_window'>
            <field name='name'>Unidades Asignadas</field>
            <field name='res_model'>chatarra.unit</field>
            <field name="view_id" ref="chatarra_unit_tree"/>
            <field name="domain">[('state','=','asignada')]</field>
            <field name='view_type'>form</field>
            <field name='view_mode'>tree,form</field>
        </record>
        <!-- Accion Chatarra/Unidades/Expediente Completo -->
        <record id='action_chatarra_expediente_completo' model='ir.actions.act_window'>
            <field name='name'>Expediente Completo</field>
            <field name='res_model'>chatarra.unit</field>
            <field name="view_id" ref="chatarra_unit_tree"/>
            <field name="domain">[('state','=','completo')]</field>
            <field name='view_type'>form</field>
            <field name='view_mode'>tree,form</field>
        </record>
        <!-- Accion Chatarra/Unidades/Enviadas a SCT -->
        <record id='action_chatarra_enviadas_sct' model='ir.actions.act_window'>
            <field name='name'>Enviadas a SCT</field>
            <field name='res_model'>chatarra.unit</field>
            <field name="view_id" ref="chatarra_unit_enviado_sct_tree"/>
            <field name="domain">[('state','=','enviado')]</field>
            <field name='view_type'>form</field>
            <field name='view_mode'>tree</field>
        </record>
        <!-- Accion Chatarra/Unidades/Recibidas de SCT -->
        <record id='action_chatarra_recibido' model='ir.actions.act_window'>
            <field name='name'>Recibidas de SCT</field>
            <field name='res_model'>chatarra.unit</field>
            <field name="view_id" ref="chatarra_unit_tree"/>
            <field name="domain">[('state','=','recibido')]</field>
